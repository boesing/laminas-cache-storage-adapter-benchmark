{
    "checks": [
        {
            "name": "Execute benchmarks on all supported PHP platforms",
            "job": {
                "php": "*",
                "dependencies": "latest",
                "command": "./vendor/bin/phpbench run --revs=2 --iterations=2 --report=aggregate"
            }
        }
    ],
    "ignore_php_platform_requirement": {
        "8.3": true
    }
}
